    <xsd:all>
      <xsd:element minOccurs="1" maxOccurs="1" name="MunicipalityCVR" type="tns:CVRNumber" />
    </xsd:all>
  </xsd:complexType>
  <xsd:simpleType name="CVRNumber">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]" />
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>